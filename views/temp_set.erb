<article class="main set">
  <h1><%= @title.empty? ? 'Untitled Set' : @title %></h1>
  <a href="/sets/temp_set/edit"><i class="material-icons">create</i>Edit Set</a>
  <table class="set">
  <% unless @cards.nil? %>
    <% @cards.each do |term, definition| %>
      <colgroup>
        <col class="term">
        <col class="definition">
      </colgroup>
      <tr>
        <td><%== term %></td>
        <td><%== definition %></td>
      </tr>
    <% end %>
    </table>
  <% end %>
</article>
